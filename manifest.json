{
  "manifest_version": 2,

  "name": "Block it!",
  "version": "1.0.0",
  "description": "Block websites from your browsers during a time frame",

  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "browser_action": {
    "default_title": "Block it!",
    "default_popup": "popup.html"
  },

  "permissions": ["storage"]
}
